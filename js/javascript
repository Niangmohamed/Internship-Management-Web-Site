function verif_form()
{
    if(document.form.nomstag.value == "")  {
    alert("Veuillez entrer votre nom !");
    document.form.nomstag.focus();
    return false;
    }     
    if(document.form.prenomstag.value == "") {
    alert("Veuillez entrer votre pr√©nom!");
    document.form.prenomstag.focus();
    return false;
    }
}
